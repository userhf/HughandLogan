  delay(100);              
  digitalWrite(13, LOW);    
  delay(1000);
  digitalWrite(13, HIGH);   //short
  delay(100);              
  digitalWrite(13, LOW);    
  delay(4000); //big space
  digitalWrite(13, HIGH);   //short
  delay(100);              
  digitalWrite(13, LOW);    
  delay(1000);
  digitalWrite(13, HIGH);   //long
  delay(1000);              
