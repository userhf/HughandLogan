  digitalWrite(13, LOW);    
  delay(1000);               
  digitalWrite(13, HIGH);   //short
  delay(100);              
  digitalWrite(13, LOW);    
  delay(1000);               
  digitalWrite(13, HIGH);   //long
  delay(1000);              
  digitalWrite(13, LOW);    
  delay(1000);
  delay(2000);  //space
  digitalWrite(13, HIGH);   //long
  delay(1000);              
  digitalWrite(13, LOW);    
  delay(1000);
  digitalWrite(13, HIGH);   //long
  delay(1000);              
  digitalWrite(13, LOW);    
  delay(1000);
  digitalWrite(13, HIGH);   //short
  delay(100);              
  digitalWrite(13, LOW);    
  delay(1000);
  delay(2000);  //space
  digitalWrite(13, HIGH);   //short
  delay(100);              
  digitalWrite(13, LOW);    
  delay(1000);
  digitalWrite(13, HIGH);   //short
  delay(100);              
  digitalWrite(13, LOW);    
  delay(1000);
